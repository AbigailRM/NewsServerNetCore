{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.0.2",
      "destination": "wwwroot/lib/bootstrap/css/",
      "files": [
        "css/bootstrap.min.css"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "grunt-flaticon@0.3.1",
      "destination": "wwwroot/flaticon/",
      "files": [
        "templates/flaticon.css"
      ]
    },
    {
      "library": "owl-carousel@1.3.3",
      "destination": "wwwroot/owlcarousel/",
      "files": [
        "owl.carousel.min.js",
        "owl.carousel.min.css"
      ]
    },
    {
      "library": "lightbox2@2.11.3",
      "destination": "wwwroot/lightbox/",
      "files": [
        "js/lightbox.min.js",
        "css/lightbox.min.css"
      ]
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/lib/animate/"
    },
    {
      "library": "font-awesome@5.15.3",
      "destination": "wwwroot/awesome/"
    },
    {
      "library": "wow@1.1.2",
      "destination": "wwwroot/lib/wow/",
      "files": [
        "wow.min.js"
      ]
    },
    {
      "library": "jquery-easing@1.4.1",
      "destination": "wwwroot/lib/easing/"
    },
    {
      "library": "jquery.isotope@3.0.6",
      "destination": "wwwroot/lib/isotope/"
    },
    {
      "library": "tinymce@5.8.2",
      "destination": "wwwroot/tinymce/"
    },
    {
      "provider": "jsdelivr",
      "library": "gulp-favicons@3.0.0",
      "destination": "wwwroot/lib/favicons/"
    }
  ]
}